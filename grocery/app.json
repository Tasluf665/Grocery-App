{
  "expo": {
    "name": "Nectar",
    "slug": "grocery-app",
    "scheme": "grocery-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tasluf.groceryapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Gilroy/Gilroy_ExtraBold.otf",
            "./assets/fonts/Gilroy/Gilroy_Light.otf",
            "./assets/fonts/Lato/Lato-Black.ttf",
            "./assets/fonts/Lato/Lato-BlackItalic.ttf",
            "./assets/fonts/Lato/Lato-Bold.ttf",
            "./assets/fonts/Lato/Lato-BoldItalic.ttf",
            "./assets/fonts/Lato/Lato-Italic.ttf",
            "./assets/fonts/Lato/Lato-Light.ttf",
            "./assets/fonts/Lato/Lato-LightItalic.ttf",
            "./assets/fonts/Lato/Lato-Regular.ttf",
            "./assets/fonts/Lato/Lato-Thin.ttf",
            "./assets/fonts/Lato/Lato-ThinItalic.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d8f6c5dc-79e4-489a-8d14-5034c1fad6d6"
      }
    },
    "owner": "tasluf",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d8f6c5dc-79e4-489a-8d14-5034c1fad6d6"
    }
  }
}
